    "import cv2\n",
    "\n",
    "def get_edge(img):\n",
    "    gray = cv2.cvtColor(img, cv2.COLOR_BGR2GRAY)\n",
    "    blur = cv2.GaussianBlur(gray, (13, 13), 0)\n",
    "    canny = cv2.Canny(blur, 50, 150)\n",
    "    return canny\n"
   ]
